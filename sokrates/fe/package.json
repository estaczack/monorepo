    "prod": "npm run build-prod && npm run start"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.18.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "sass": "^1.62.1",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1"
  }
}
